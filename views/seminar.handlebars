<div class="home">
  <div class="parallax_background parallax-window" data-parallax="scroll" data-image-src="/images/properties.jpg" data-speed="0.8"></div>
  <div class="home_container">
    <div class="container">
      <div class="row">
        <div class="col">
          <div class="home_content d-flex flex-row align-items-end justify-content-start">
            <div class="home_title">Explore Seminars</div>
            <div class="breadcrumbs ml-auto">
              <ul>
                <li><a href="/">Home</a></li>
                <li>Explore Seminars</li>
              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Home Search -->
<div class="home_search">
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="home_search_container">
          <div class="home_search_content">
            <form action="seminar" class="search_form d-flex flex-row align-items-start justfy-content-start" method="get">
              <div class="search_form_content d-flex flex-row align-items-start justfy-content-start flex-wrap">
                <div>
                  <select class="search_form_select" name="seminar_gamename">
                    <option disabled selected>Game</option>
                    <option value="pubg">Pubg</option>
                    <option value="candycrush">Candy Crush</option>
                    <option value="fortnite">Fortnite</option>
                    <option value="cs">Counter Strike</option>
                  </select>
                </div>
                <div>
                  <select class="search_form_select" name="seminar_gametype">
                    <option disabled selected>Game Type</option>
                    <option value="racing">Racing</option>
                    <option value="shooting">Shooting</option>
                    <option value="strategy">Strategy</option>
                    <option value="puzzle">Puzzle</option>
                    <option value="role-playing">Role Playing</option>
                    <option value="arcade">Arcade</option>
                    <option value="action">Action</option>
                  </select>
                </div>
                <div>
                  <select class="search_form_select" name="seminar_price">
                    <option disabled selected>Seminar Price</option>
                    <option value="1">High to Low</option>
                    <option value="-1">Low to High</option>
                  </select>
                </div>
                <div>
                  <select class="search_form_select" name="seminar_country">
                    <option disabled selected>Country</option>
                    <option value="india">India</option>
                    <option value="usa">USA</option>
                    <option value="sri-lanka">Sri Lanka</option>
                    <option value="china">China</option>
                    <option value="russia">Russia</option>
                    <option value="south-korea">South Korea</option>
                    <option value="england">England</option>
                    <option value="malaysia">Malaysia</option>
                  </select>
                </div>
                <div>
                  <select class="search_form_select" name="seminar_state">
                    <option disabled selected>State</option>
                    <option value="rajasthan">Rajasthan</option>
                    <option value="maharashtra">Maharashtra</option>
                    <option value="kerala">Kerala</option>
                    <option value="karnataka">Karnataka</option>
                    <option value="goa">Goa</option>
                    <option value="punjab">Punjab</option>
                    <option value="haryana">Haryana</option>
                    <option value="gujarat">Gujarat</option>
                  </select>
                </div>
                <div>
                  <select class="search_form_select" name="seminar_city">
                    <option disabled selected>City</option>
                    <option value="mumbai">Mumbai</option>
                    <option value="pune">Pune</option>
                    <option value="kolkata">Kolkata</option>
                    <option value="jaipur">jaipur</option>
                    <option value="panji">Panji</option>
                    <option value="chandigarh">Chandigarh</option>
                    <option value="bangalore">Bangalore</option>
                    <option value="hyderabad">Hyderabad</option>
                  </select>
                </div>
              </div>
              <button class="search_form_button ml-auto" type="submit">search</button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- Properties -->

<div class="properties">
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="section_title">{{seminarCount}} Seminars found</div>
        <div class="section_subtitle">Become a great Gamer by joining the seminars</div>
      </div>
    </div>
    <div class="row properties_row">

      <!-- Property -->
      {{#if seminar}}
      {{#each seminar}}

      <div class="col-xl-4 col-lg-6 property_col">
        <div class="property">
          <div class="property_image">
            <img src="/uploads/{{seminar_image}}" alt="" height="200" width="100%">
            <div class="tag_featured property_tag"><a href="#">Featured</a></div>
          </div>
          <div class="property_body text-center">
            <div class="property_location">{{seminar_gametype}}</div>
            <div class="property_title"><a href="#">{{seminar_title}}</a></div>
            <div class="button recent_button"><a href="/seminar/detail/{{id}}">Join Seminar</a></div>
          </div>
          <div class="property_footer d-flex flex-row align-items-center justify-content-center">
            <div>{{seminar_city}}</div>
            <div><div class="property_icon"><img src="/images/event-calender.png" alt=""></div><span>24/07/2019</span></div>
            <!-- <div><div class="property_icon"><img src="/images/icon_2.png" alt=""></div><span>3 Bedrooms</span></div> -->
            <div><span>â‚¹ &nbsp;{{seminar_price}}</span></div>
          </div>
        </div>
      </div>
      {{/each}}
      {{/if}}
