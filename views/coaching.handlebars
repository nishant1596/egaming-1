<!-- Properties -->

<div class="properties">
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="section_title">{{coachingCount}} coachings found</div>
        <div class="section_subtitle">Become a great Gamer by joining the coachings</div>
      </div>
    </div>
    <div class="row properties_row">

      <!-- Property -->
      {{#if coaching}}
      {{#each coaching}}

      <div class="col-xl-4 col-lg-6 property_col">
        <div class="property">
          <div class="property_image">
            <img src="/uploads/1557235108603-cod.jpg" alt="" height="200" width="100%">
            {{#ifCond "on" coaching_isPremium}}
            <div class="tag_featured property_tag"><a href="#">Featured</a></div>
            {{/ifCond}}
          </div>
          <div class="property_body text-center">
            <div class="property_location">{{coaching_type}}</div>
            <div class="property_title mb-3"><a href="#">{{coaching_title}}</a></div>
            <div class="button recent_button"><a href="/coaching/detail/{{id}}">view coaching</a></div>
          </div>
        </div>
      </div>
      {{/each}}
      {{/if}}
      </div>
    </div>
      <div class="container text-center" >
        <div class="row">
          <div class="col">
            <div class="pagination">
              <ul>
                Jump To:
                {{#times pages}}
                  <li class="active" style="border:1px solid pink; padding:5px 10px;margin-right:10px;"><a href="/coaching/{{this}}">{{this}}</a></li>
                {{/times}}
              </ul>
            </div>
          </div>
        </div>
      </div>



  </div>
</div>

<!-- Newsletter -->

<div class="newsletter">
  <div class="parallax_background parallax-window" data-parallax="scroll" data-image-src="/images/newsletter.jpg" data-speed="0.8"></div>
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="newsletter_content d-flex flex-lg-row flex-column align-items-start justify-content-start">
          <div class="newsletter_title_container">
            <div class="newsletter_title">Are you buying or selling?</div>
            <div class="newsletter_subtitle">Search your dream home</div>
          </div>
          <div class="newsletter_form_container">
            <form action="#" class="newsletter_form">
              <input type="email" class="newsletter_input" placeholder="Your e-mail address" required="required">
              <button class="newsletter_button">subscribe now</button>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

