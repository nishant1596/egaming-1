<link rel="stylesheet" href="/styles/mycss.css">

<center>
  <div class="front-ui-background container" style="margin-top:100px;">
    <div class="">
      <img src="/images/background1.png" class="e-gaming-logo">
    </div>
    <div class="">
      <p id="home-gif"></p>
    </div>
    <div class="">
      <img src="/images/gamepad.png" class="e-gaming-image"><br>
      <a href="#section2" class="btn-floating white pulse">
      </a>
    </div>
  </div>
</center>
<!-- Pulse Buton -->
<div align="center" style="margin-top:-200px;">
  <div class="container">
    <button class="pulse-button"></button>
  </div>
</div>
<!-- Pulse Buton finishes -->

<!--Seminar section starts -->
	<div class="recent">
		<div class="container">
			<div class="row">
				<div class="col">
					<div class="section_title">Recent Seminars</div>
					<div class="section_subtitle">The "About Seminar Line"</div>
				</div>
			</div>
			<div class="row recent_row">
				<div class="col">
					<div class="recent_slider_container">
						<div class="owl-carousel owl-theme recent_slider">
              {{#if seminarfetched6}}
              {{#each seminarfetched6}}
							<!-- Slide -->
							<div class="owl-item">
								<div class="recent_item">
									<div class="recent_item_inner">
										<div class="recent_item_image">
                      <img src="/uploads/{{seminar_image}}" alt="" height="200px">
											<div class="tag_featured property_tag"><a href="#">Featured</a></div>
										</div>
										<div class="recent_item_body text-center">
											<div class="recent_item_location">{{seminar_gametype}}</div>
											<div class="recent_item_title"><a href="property.html">{{seminar_title}}</a></div>
											<div class="button recent_button"><a href="/seminar/detail/{{id}}">Join Seminar</a></div>
										</div>
										<div class="recent_item_footer d-flex flex-row align-items-center justify-content-center">
											<div><span>{{seminar_city}}</span></div>
											<div><div class="recent_icon"><img src="/images/event-calender.png" alt=""></div><span>21/05/2019</span></div>
                      <div><span>₹ {{seminar_price}}</span></div>
											<!-- <div><div class="recent_icon"><img src="images/icon_3.png" alt=""></div><span>3 Bathrooms</span></div> -->
										</div>
									</div>
								</div>
							</div>
              {{/each}}
              {{/if}}
        	</div>
						<div class="recent_slider_nav_container d-flex flex-row align-items-start justify-content-start">
							<div class="recent_slider_nav recent_slider_prev"><i class="fa fa-chevron-left" aria-hidden="true"></i></div>
							<div class="recent_slider_nav recent_slider_next"><i class="fa fa-chevron-right" aria-hidden="true"></i></div>
						</div>
					</div>
					<div class="button recent_button"><a href="/seminar/1">see more</a></div>
				</div>
			</div>
		</div>
	</div>
<!--Seminar Section Finishes-->


